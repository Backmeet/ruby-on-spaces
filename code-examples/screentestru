window init 900 600

var new quit 0

var new y 600
var new temp 0

while begin

    window draw line 0 y 900 y 255 255 255 10
    
    var math y - 1 = y

    var math y == 0 = temp
    if begin temp
        var set y = 600
    if end

    window flip
    window fill 0 0 0

    window update
    window event quit quit
    var math quit not = quit
while end quit
window quit

while begin
print "other task"
while end 1

end
    
